{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [],
    "watchAssets": true
  },
  "projects": {
    "worker": {
      "type": "application",
      "root": ".",
      "entryFile": "worker",
      "sourceRoot": "src"
    }
  }
}
